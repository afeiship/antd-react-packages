{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*",
    "!packages/websites"
  ],
  "scripts": {
    "dev": "cd packages/antd-components && yarn dev",
    "start": "cd packages/websites && npm run start",
    "build": "lerna run build --since master --no-private",
    "build:all": "lerna run build --parallel",
    "publish": "npm run build && lerna publish --no-private"
  },
  "devDependencies": {
    "lerna": "^7.2.0"
  }
}
